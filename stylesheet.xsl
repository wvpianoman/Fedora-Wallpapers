   <!-- show-contactInfo -->
   <xsl:template name="show-contactInfo">
      <xsl:param name="contact"/>
      <xsl:param name="showaddresstelecomcode"/>
      <xsl:param name="showphonetelecomcode"/>
     <xsl:param name="showallnumbers"/>
      <xsl:call-template name="show-address">
         <xsl:with-param name="address" select="$contact/n1:addr[1]"/>
         <xsl:with-param name="showtelecomcode" select="$showaddresstelecomcode"/>
      </xsl:call-template>
      <xsl:if test="$contact/n1:telecom/@value">
         <xsl:if test="$contact/n1:addr/*[text() and text() !='US']">
            <xsl:text>,&#160;</xsl:text>
         </xsl:if>
         <xsl:text>Ph. </xsl:text>
         <xsl:call-template name="show-telecom">
            <xsl:with-param name="telecom" select="$contact/n1:telecom"/>
            <xsl:with-param name="showtelecomcode" select="$showphonetelecomcode"/>
         </xsl:call-template>
      </xsl:if>
      <xsl:text>&#160;</xsl:text>
   </xsl:template>
   <!-- show-contactInfo-otheraddresses -->
   <xsl:template name="show-contactInfo-otheraddresses">
      <xsl:param name="contact"/>
      <xsl:for-each select="$contact/n1:addr">
        <xsl:variable name="index" select="position()"/>
        <xsl:if test="$index != 1">
            <xsl:call-template name="show-other-address">
                <xsl:with-param name="address" select="$contact/n1:addr[$index]"/>
            </xsl:call-template>
            <br/>
        </xsl:if>
        <xsl:if test="$index = 1 and position()=last()">
            <xsl:text> Patient has no other address </xsl:text>
        </xsl:if>
      </xsl:for-each>
   </xsl:template>
   <!-- show-address -->
   <xsl:template name="show-address">
      <xsl:param name="address"/>
      <xsl:param name="showtelecomcode" />      
<!-- Only show address if there is non-empty text in one of the child elements -->
      <xsl:if test="$address/*[text() and text()!='US']">
         <xsl:if test="$showtelecomcode = 1">
            <xsl:if test="$address/@use">
               <xsl:text> </xsl:text>
               <xsl:call-template name="translateTelecomCode">
                  <xsl:with-param name="code" select="$address/@use"/>
               </xsl:call-template>
               <br/>
            </xsl:if>
         </xsl:if>
	 <xsl:if test="string-length($address/n1:streetAddressLine)>0">
            <xsl:for-each select="$address/n1:streetAddressLine">
               <xsl:if test='string-length(string(.))!=0'>
                  <xsl:if test="position()!=1">
                     <xsl:text>, </xsl:text>
                  </xsl:if>
                  <xsl:value-of select="."/>
               </xsl:if>
               <xsl:if test="position()=last() and string-length($address/n1:city)!=0">
                  <xsl:text>, </xsl:text>
               </xsl:if>
            </xsl:for-each>
	 </xsl:if>
         <xsl:if test="$address/n1:streetName">
            <xsl:value-of select="$address/n1:streetName"/>
            <xsl:text>&#160;</xsl:text>
            <xsl:value-of select="$address/n1:houseNumber"/>
            <xsl:text>,&#160;</xsl:text>
         </xsl:if>
         <xsl:if test="string-length($address/n1:city)>0">
            <xsl:value-of select="$address/n1:city"/>
         </xsl:if>
         <xsl:if test="string-length($address/n1:state)>0">
            <xsl:if test="string-length($address/n1:city)>0">
               <xsl:text>,&#160;</xsl:text>
            </xsl:if>
            <xsl:value-of select="$address/n1:state"/>
         </xsl:if>
         <xsl:if test="string-length($address/n1:postalCode)>0">
            <xsl:text>&#160;</xsl:text>
            <xsl:value-of select="$address/n1:postalCode"/>
         </xsl:if>
         <xsl:if test="string-length($address/n1:country)>0 and $address/n1:country!='US'">
            <xsl:text>,&#160;</xsl:text>
            <xsl:value-of select="$address/n1:country"/>
         </xsl:if>
      </xsl:if>
   </xsl:template>
   <!-- show-other-address -->
   <xsl:template name="show-other-address">
      <xsl:param name="address"/>
      <!-- Only show address if there is non-empty text in one of the child elements -->
    <xsl:if test="$address/*[text() and text()!='US']">
        <xsl:if test="string-length($address/n1:streetAddressLine)>0">
                <xsl:for-each select="$address/n1:streetAddressLine">
                <xsl:if test='string-length(string(.))!=0'>
                    <xsl:if test="position()!=1">
                        <xsl:text>, </xsl:text>
                    </xsl:if>
                    <xsl:value-of select="."/>
                </xsl:if>
                <xsl:if test="position()=last() and string-length($address/n1:city)!=0">
                    <xsl:text>, </xsl:text>
                </xsl:if>
                </xsl:for-each>
        </xsl:if>
        <xsl:if test="$address/n1:streetName">
            <xsl:value-of select="$address/n1:streetName"/>
            <xsl:text>&#160;</xsl:text>
            <xsl:value-of select="$address/n1:houseNumber"/>
            <xsl:text>,&#160;</xsl:text>
        </xsl:if>
        <xsl:if test="string-length($address/n1:city)>0">
            <xsl:value-of select="$address/n1:city"/>
        </xsl:if>
        <xsl:if test="string-length($address/n1:state)>0">
            <xsl:if test="string-length($address/n1:city)>0">
               <xsl:text>,&#160;</xsl:text>
            </xsl:if>
            <xsl:value-of select="$address/n1:state"/>
        </xsl:if>
        <xsl:if test="string-length($address/n1:postalCode)>0">
            <xsl:text>&#160;</xsl:text>
            <xsl:value-of select="$address/n1:postalCode"/>
        </xsl:if>
        <xsl:if test="string-length($address/n1:country)>0 and $address/n1:country!='US'">
            <xsl:text>,&#160;</xsl:text>
            <xsl:value-of select="$address/n1:country"/>
        </xsl:if>
        <xsl:choose>
            <xsl:when test="$address/n1:useablePeriod/n1:high and $address/@use='HP'">
                <xsl:text> (Previous Home Address) </xsl:text>
            </xsl:when>
            <xsl:when test="$address/n1:useablePeriod/n1:high and $address/@use='PST'">
                <xsl:text> (Previous Billing Address) </xsl:text>
            </xsl:when>
            <xsl:when test="$address/n1:useablePeriod/n1:high and $address/@use='TMP'">
                <xsl:text> (Previous Temporary Address) </xsl:text>
            </xsl:when>
            <xsl:when test="$address/n1:useablePeriod/n1:high and $address/@use='WP'">
                <xsl:text> (Previous Work Address) </xsl:text>
            </xsl:when>
            <xsl:when test="$address/n1:useablePeriod/n1:high">
                <xsl:text> (Previous Address) </xsl:text>
            </xsl:when>
            <xsl:when test="$address/n1:useablePeriod/n1:low and $address/@use='HP'">
                <xsl:text> (Current Home Address) </xsl:text>
            </xsl:when>
            <xsl:when test="$address/n1:useablePeriod/n1:low and $address/@use='WP'">
                <xsl:text> (Current Work Address) </xsl:text>
            </xsl:when>
            <xsl:when test="$address/n1:useablePeriod/n1:low and $address/@use='PST'">
                <xsl:text> (Current Billing Address) </xsl:text>
            </xsl:when>
            <xsl:when test="$address/n1:useablePeriod/n1:low and $address/@use='TMP'">
                <xsl:text> (Current Temporary Address) </xsl:text>
            </xsl:when>
            <xsl:when test="$address/n1:useablePeriod/n1:low">
                <xsl:text> (Current Address) </xsl:text>
            </xsl:when>
            <xsl:otherwise></xsl:otherwise>
        </xsl:choose>
      </xsl:if>
   </xsl:template>
   <!-- show-telecom -->
   <xsl:template name="show-telecom">
      <xsl:param name="telecom"/>
      <xsl:param name="showtelecomcode" />
      <xsl:choose>
         <xsl:when test="$telecom">
         <xsl:for-each select="$telecom/@use">
            <xsl:variable name="index" select="position()"/>
            <xsl:variable name="value" select="$telecom/@value"/>
            <xsl:variable name="use" select="$telecom/@use"/>
            <xsl:value-of select="$value[$index]"/>
            <xsl:if test="$showtelecomcode = 1">
              <xsl:if test="$telecom/@use">
                <xsl:text> </xsl:text>
                <xsl:call-template name="translateTelecomCode">
                  <xsl:with-param name="code" select="$use[$index]"/>
                </xsl:call-template>
              </xsl:if>
             </xsl:if>
            <xsl:text> </xsl:text>            
         </xsl:for-each>
         </xsl:when>
         <xsl:otherwise>
            <xsl:text>Telecom information not available</xsl:text>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>
   <!-- show-recipientType -->
   <xsl:template name="show-recipientType">
      <xsl:param name="typeCode"/>
      <xsl:choose>
         <xsl:when test="$typeCode='PRCP'">Primary Recipient:</xsl:when>
         <xsl:when test="$typeCode='TRC'">Secondary Recipient:</xsl:when>
         <xsl:otherwise>Recipient:</xsl:otherwise>
      </xsl:choose>
   </xsl:template>
   <!-- Convert Telecom URL to display text -->
   <xsl:template name="translateTelecomCode">
      <xsl:param name="code"/>
      <!--xsl:value-of select="document('voc.xml')/systems/system[@root=$code/@codeSystem]/code[@value=$code/@code]/@displayName"/-->
      <!--xsl:value-of select="document('codes.xml')/*/code[@code=$code]/@display"/-->
      <xsl:choose>
         <!-- lookup table Telecom URI -->
         <xsl:when test="$code='tel'">
            <xsl:text>(Tel)</xsl:text>
         </xsl:when>
         <xsl:when test="$code='fax'">
            <xsl:text>(Fax)</xsl:text>
         </xsl:when>
         <xsl:when test="$code='http'">
            <xsl:text>(Web)</xsl:text>
         </xsl:when>
         <xsl:when test="$code='mailto'">
            <xsl:text>(Mail)</xsl:text>
         </xsl:when>
         <xsl:when test="$code='H'">
            <xsl:text>(Home)</xsl:text>
         </xsl:when>
         <xsl:when test="$code='HV'">
            <xsl:text>(Vacation Home)</xsl:text>
         </xsl:when>
         <xsl:when test="$code='HP'">
            <xsl:text>(Primary Home)</xsl:text>
         </xsl:when>
         <xsl:when test="$code='MC'">
            <xsl:text>(Mobile)</xsl:text>
         </xsl:when>
         <xsl:when test="$code='WP'">
            <xsl:text>(Work Place)</xsl:text>
         </xsl:when>
         <xsl:when test="$code='PUB'">
            <xsl:text>(Pub)</xsl:text>
         </xsl:when>
         <xsl:otherwise>
            <xsl:text>{$code='</xsl:text>
            <xsl:value-of select="$code"/>
            <xsl:text>'?}</xsl:text>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>
   <!-- show admit and discharge date -->
   <xsl:template name="show-admit-discharge-date">
      <xsl:param name="datetime"/>
      <xsl:if test="count($datetime/n1:low)!=0">
         <xsl:text>Admission Date : </xsl:text>
         <xsl:call-template name="formatDate">
            <xsl:with-param name="date" select="$datetime/n1:low/@value"/>
         </xsl:call-template>
         <br/>
      </xsl:if>
      <xsl:if test="count($datetime/n1:high)!=0">
         <xsl:text>Discharge Date : </xsl:text>
         <xsl:call-template name="formatDate">
            <xsl:with-param name="date" select="$datetime/n1:high/@value"/>
         </xsl:call-template>
      </xsl:if>
   </xsl:template>
   <!-- show date -->
   <xsl:template name="show-date">
      <xsl:param name="datetime"/>
      <xsl:choose>
         <xsl:when test="not($datetime)">
            <xsl:call-template name="formatDate">
               <xsl:with-param name="date" select="@value"/>
            </xsl:call-template>
            <xsl:text> </xsl:text>
         </xsl:when>
         <xsl:otherwise>
            <xsl:call-template name="formatDate">
               <xsl:with-param name="date" select="$datetime/@value"/>
            </xsl:call-template>
            <xsl:text> </xsl:text>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>
   <!-- show assignedEntity -->
   <xsl:template name="show-assignedEntity">
      <xsl:param name="asgnEntity"/>
      <xsl:choose>
         <xsl:when test="string-length($asgnEntity/n1:assignedPerson/n1:name) != 0 or string-length($asgnEntity/n1:assignedPerson/n1:name/n1:family) != 0">
            <xsl:call-template name="show-name">
               <xsl:with-param name="name" select="$asgnEntity/n1:assignedPerson/n1:name"/>
            </xsl:call-template>
            <xsl:if test="string-length($asgnEntity/n1:representedOrganization/n1:name) != 0">
               <xsl:text>, </xsl:text>
               <xsl:value-of select="$asgnEntity/n1:representedOrganization/n1:name"/>
            </xsl:if>
         </xsl:when>
         <xsl:when test="string-length($asgnEntity/n1:representedOrganization/n1:name) != 0">
            <xsl:value-of select="$asgnEntity/n1:representedOrganization/n1:name"/>
         </xsl:when>
          <xsl:otherwise>
            <xsl:text>None Recorded</xsl:text>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>
   <xsl:template name="formatDate">
      <xsl:param name="date"/>
      <xsl:if test="string-length($date)!=0">
         <!-- month -->
         <xsl:variable name="month" select="substring ($date, 5, 2)"/>
         <xsl:value-of select="$month"/>

         <xsl:text>/</xsl:text>
         <!-- day -->
         <xsl:value-of select="substring ($date, 7, 2)"/>
         <xsl:text>/</xsl:text>

         <!-- year -->
         <xsl:value-of select="substring ($date, 1, 4)"/>
      </xsl:if>
   </xsl:template>
   <!-- convert to lower case -->
   <xsl:template name="caseDown">
      <xsl:param name="data"/>
      <xsl:if test="$data">
         <xsl:value-of select="translate($data, 'ABCDEFGHIJKLMNOPQRSTUVWXYZ','abcdefghijklmnopqrstuvwxyz')"/>
      </xsl:if>
   </xsl:template>
   <!-- convert to upper case -->
   <xsl:template name="caseUp">
      <xsl:param name="data"/>
      <xsl:if test="$data">
         <xsl:value-of select="translate($data,'abcdefghijklmnopqrstuvwxyz', 'ABCDEFGHIJKLMNOPQRSTUVWXYZ')"/>
      </xsl:if>
   </xsl:template>
   <!-- convert first character to upper case -->
   <xsl:template name="firstCharCaseUp">
      <xsl:param name="data"/>
      <xsl:if test="$data">
         <xsl:call-template name="caseUp">
            <xsl:with-param name="data" select="substring($data,1,1)"/>
         </xsl:call-template>
         <xsl:value-of select="substring($data,2)"/>
      </xsl:if>
   </xsl:template>
   <!-- show-noneFlavor -->
   <xsl:template name="show-noneFlavor">
      <xsl:param name="nf"/>
      <xsl:choose>
         <xsl:when test=" $nf = 'NI' ">
            <xsl:text>no information</xsl:text>
         </xsl:when>
         <xsl:when test=" $nf = 'INV' ">
            <xsl:text>invalid</xsl:text>
         </xsl:when>
         <xsl:when test=" $nf = 'MSK' ">
            <xsl:text>masked</xsl:text>
         </xsl:when>
         <xsl:when test=" $nf = 'NA' ">
            <xsl:text>not applicable</xsl:text>
         </xsl:when>
         <xsl:when test=" $nf = 'UNK' ">
            <xsl:text>unknown</xsl:text>
         </xsl:when>
         <xsl:when test=" $nf = 'OTH' ">
            <xsl:text>other</xsl:text>
         </xsl:when>
      </xsl:choose>
   </xsl:template>
   <xsl:template name="show-unavailable-header">
      <xsl:text>Information not available</xsl:text>
   </xsl:template>
