   <!-- line break for paragraph tag-->
   <xsl:template name="insertBreaks">
      <xsl:param name="pText" select="."/>
      <xsl:param name="lineBreak"/>
      <xsl:variable name="maxLineBreak" select="239"/>
      <xsl:choose>
         <xsl:when test="not(contains($pText, '&#xA;')) or $lineBreak &gt; $maxLineBreak">
            <xsl:copy-of select="$pText"/>
         </xsl:when>
         <xsl:otherwise>
            <xsl:value-of select="substring-before($pText, '&#xA;')"/>
            <br />
            <xsl:call-template name="insertBreaks">
               <xsl:with-param name="pText" select="substring-after($pText, '&#xA;')"/>
               <xsl:with-param name="lineBreak" select="$lineBreak + 1"/>
            </xsl:call-template>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>
