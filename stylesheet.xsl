   <!--  show-id -->
   <xsl:template name="show-id">
      <xsl:param name="id"/>
      <xsl:choose>
         <xsl:when test="not($id)">
            <xsl:if test="not(@nullFlavor)">
               <xsl:if test="@extension">
                  <xsl:value-of select="@extension"/>
               </xsl:if>
               <xsl:text> </xsl:text>
               <xsl:value-of select="@root"/>
            </xsl:if>
         </xsl:when>
         <xsl:otherwise>
            <xsl:if test="not($id/@nullFlavor)">
               <xsl:if test="$id/@extension">
                  <xsl:value-of select="$id/@extension"/>
               </xsl:if>
               <xsl:text> </xsl:text>
               <xsl:value-of select="$id/@root"/>
            </xsl:if>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>
   <!-- show-name  -->
   <xsl:template name="show-name">
      <xsl:param name="name"/>
      <xsl:choose>
         <xsl:when test="$name/n1:family">
            <xsl:if test="$name/n1:prefix">
               <xsl:value-of select="$name/n1:prefix"/>
               <xsl:text> </xsl:text>
            </xsl:if>
            <xsl:for-each select="$name/n1:given">
               <xsl:value-of select="text()"/>
               <xsl:text> </xsl:text>
            </xsl:for-each>
            <xsl:value-of select="$name/n1:family"/>
            <xsl:if test="$name/n1:suffix">
               <xsl:text>, </xsl:text>
               <xsl:value-of select="$name/n1:suffix"/>
            </xsl:if>
         </xsl:when>
         <xsl:when test="$name/n1:name[@use='L']">
            <xsl:call-template name="show-name-inner">
               <xsl:with-param name="namenode" select="$name/n1:name[@use='L']"/>
            </xsl:call-template>
         </xsl:when>
         <xsl:when test="$name/n1:name[@use='SRCH']">
            <xsl:call-template name="show-name-inner">
               <xsl:with-param name="namenode" select="$name/n1:name[@use='SRCH']"/>
            </xsl:call-template>
         </xsl:when>
         <xsl:when test="$name/n1:name[@use='C']">
            <xsl:call-template name="show-name-inner">
               <xsl:with-param name="namenode" select="$name/n1:name[@use='C']"/>
            </xsl:call-template>
         </xsl:when>
         <xsl:when test="$name/n1:name">
            <xsl:call-template name="show-name-inner">
               <xsl:with-param name="namenode" select="$name/n1:name"/>
            </xsl:call-template>
         </xsl:when>
         <xsl:otherwise>
            <xsl:value-of select="$name"/>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>
   <!-- show-name-inner  -->
   <xsl:template name="show-name-inner">
      <xsl:param name="namenode"/>
      <xsl:if test="$namenode/n1:prefix">
         <xsl:value-of select="$namenode/n1:prefix"/>
         <xsl:text> </xsl:text>
      </xsl:if>
      <xsl:for-each select="$namenode/n1:given">
         <xsl:value-of select="text()"/>
         <xsl:text> </xsl:text>
      </xsl:for-each>
      <xsl:value-of select="$namenode/n1:family"/>
      <xsl:if test="$namenode/n1:suffix">
         <xsl:text>, </xsl:text>
         <xsl:value-of select="$namenode/n1:suffix"/>
      </xsl:if>
   </xsl:template>
   <!-- show-previous-name -->
   <xsl:template name="show-previous-name">
      <xsl:param name="name"/>
      <!--xsl:for-each select="n1:item"-->
      <!-- So some explanation Needed here for clarity:
            There is an assumption that for each name object, there will be one type associated with it (birth,academic, etc).
            Based on that assumption the code below looks for any name with a qualifer attached, and prints the label associated with it.
            This code is going to exit ON THE FIRST match of a qualifier (according to the assumption there should only be one)
      -->
      <xsl:choose>
         <xsl:when test="$name/n1:*/@qualifier = &apos;AC&apos; ">
            <xsl:text> Academic: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
         <xsl:when test="$name/n1:*/@qualifier = &apos;AD&apos; ">
            <xsl:text> Adopted: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
         <xsl:when test="$name/n1:*/@qualifier = &apos;BR&apos; ">
            <xsl:text> Birth: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
         <xsl:when test="$name/n1:*/@qualifier = &apos;CL&apos; ">
            <xsl:text> Call Me: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
         <xsl:when test="$name/n1:*/@qualifier = &apos;LS&apos; ">
            <xsl:text> Legal Status: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
         <xsl:when test="$name/n1:*/@qualifier = &apos;NB&apos; ">
            <xsl:text> Nobility: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
         <xsl:when test="$name/n1:*/@qualifier = &apos;PR&apos; ">
            <xsl:text> Professional: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
         <xsl:when test="$name/n1:*/@qualifier = &apos;SP&apos; ">
            <xsl:text> Spouse: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
         <xsl:when test="$name/n1:*/@qualifier = &apos;TITLE&apos; ">
            <xsl:text> Title: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
         <xsl:when test="$name/n1:*/@qualifier = &apos;VV&apos; ">
            <xsl:text> Voorvoegsel: </xsl:text>
            <xsl:value-of select="$name"/>
         </xsl:when>
      </xsl:choose>
   </xsl:template>
