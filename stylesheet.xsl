<xsl:stylesheet version="1.0" xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:n1="urn:hl7-org:v3" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:sdtc="urn:hl7-org:sdtc">
   <xsl:output method="html" indent="yes" version="4.01" encoding="UTF-8" doctype-system="http://www.w3.org/TR/html4/strict.dtd" doctype-public="-//W3C//DTD HTML 4.01//EN"/>
   <!-- global variable title -->
   <xsl:variable name="title">
      <xsl:choose>
         <xsl:when test="string-length(/n1:ClinicalDocument/n1:title)  &gt;= 1">
            <xsl:value-of select="/n1:ClinicalDocument/n1:title"/>
         </xsl:when>
         <xsl:when test="/n1:ClinicalDocument/n1:code/@displayName">
            <xsl:value-of select="/n1:ClinicalDocument/n1:code/@displayName"/>
         </xsl:when>
         <xsl:otherwise>
            <xsl:text>Clinical Document</xsl:text>
         </xsl:otherwise>
      </xsl:choose>
      <xsl:text> for </xsl:text>
      <xsl:call-template name="show-name">
         <!-- Based on the fact that only the first name should be used in the title we'll select only the first name-->
         <xsl:with-param name="name" select="/n1:ClinicalDocument/n1:recordTarget/n1:patientRole/n1:patient/n1:name[1]"/>
      </xsl:call-template>
   </xsl:variable>
   <!-- Main -->
   <xsl:template match="/">
      <xsl:apply-templates select="n1:ClinicalDocument"/>
   </xsl:template>
   <!-- produce browser rendered, human readable clinical document -->
   <xsl:template match="n1:ClinicalDocument">
      <html>
         <head>
            <xsl:comment> Do NOT edit this HTML directly: it was generated via an XSLT transformation from a CDA Release 2 XML document. </xsl:comment>
            <title>
               <xsl:value-of select="$title"/>
            </title>
            <xsl:call-template name="addCSS"/>
         </head>
         <body>
            <div id="CLINICALSUMMARYTITLE">
               <xsl:value-of select="$title"/>
            </div>
            <!-- START display top portion of clinical document -->

            <hr/>
            <xsl:if test="count(n1:componentOf/n1:encompassingEncounter) &gt; 0">
               <xsl:call-template name="mostrecentencounter"/>
               <hr/>
            </xsl:if>
            <!-- END display top portion of clinical document -->
            <!-- produce table of contents -->
            <xsl:if test="not(//n1:nonXMLBody)">
               <xsl:if test="count(/n1:ClinicalDocument/n1:component/n1:structuredBody/n1:component[n1:section]) &gt; 1">
                  <xsl:call-template name="make-tableofcontents"/>
               </xsl:if>
            </xsl:if>
            <hr/>
            <!-- pull ds4p section, if present, to the top -->
            <xsl:if test="not(//n1:nonXMLBody)">
               <xsl:if test="count(/n1:ClinicalDocument/n1:component/n1:structuredBody/n1:component/n1:section[.//*/@root = '2.16.840.1.113883.3.3251.1.5']) &gt; 0">
                  <xsl:for-each select="/n1:ClinicalDocument/n1:component/n1:structuredBody/n1:component/n1:section[.//*/@root = '2.16.840.1.113883.3.3251.1.5']">
                     <xsl:call-template name="section"/>
                     <hr/>
                  </xsl:for-each>
               </xsl:if>
            </xsl:if>
            <!-- produce header sections -->
            <hr/>
            <xsl:call-template name="demographics"/>
            <xsl:if test="count(n1:documentationOf/n1:serviceEvent/n1:performer) &gt; 0">
               <hr/>
               <xsl:call-template name="careteammembers"/>
            </xsl:if>
            <xsl:if test="count(/n1:ClinicalDocument/n1:legalAuthenticator) &gt; 0">
               <hr/>
               <xsl:call-template name="legalauthenticator"/>
            </xsl:if>
            <hr/>
            <!-- produce human readable document content -->
            <xsl:apply-templates select="n1:component/n1:structuredBody|n1:component/n1:nonXMLBody"/>

            <br/>
            <br/>
         </body>
      </html>
   </xsl:template>
   <!-- generate table of contents -->
   <xsl:template name="make-tableofcontents">
      <div class="ccdasection">
         <span class="chartexportsectionheader" section="Table of Contents">Table of Contents</span>
         <ul class="chartexporttoc">
            <li>
               <a href="#patientdemographics">Demographics</a>
            </li>
            <xsl:if test="count(n1:documentationOf/n1:serviceEvent/n1:performer) &gt; 0">
               <li>
                  <a href="#careteammembers">Care Team Members</a>
               </li>
            </xsl:if>
            <xsl:if test="count(/n1:ClinicalDocument/n1:legalAuthenticator) &gt; 0">
             <li>
               <a href="#legalauthenticator">LegalAuthenticator</a>
            </li>
            </xsl:if>
            <xsl:for-each select="n1:component/n1:structuredBody/n1:component/n1:section/n1:title">
               <li>
                  <a href="#{generate-id(.)}">
                     <xsl:value-of select="."/>
                  </a>
               </li>
            </xsl:for-each>
         </ul>
      </div>
   </xsl:template>
   <!-- demographics -->
   <xsl:template name="demographics">
      <div class="ccdasection">
         <span class="chartexportsectionheader" section="Demographics">
            <a name="patientdemographics">Demographics</a>
         </span>
         <table class="chartexporttable" cellSpacing="0">
            <tbody>
               <xsl:for-each select="/n1:ClinicalDocument/n1:recordTarget/n1:patientRole">
                  <tr>
                     <td class="chartexportbackground">
                        <span class="bold"><xsl:text>Sex:</xsl:text></span>
                     </td>
                     <td class="chartexportbackground">
                        <xsl:choose>
                           <xsl:when test="n1:patient/n1:administrativeGenderCode">
                              <xsl:for-each select="n1:patient/n1:administrativeGenderCode">
                                 <xsl:call-template name="show-gender"/>
                              </xsl:for-each>
                           </xsl:when>
                           <xsl:otherwise>
                              <xsl:call-template name="show-unavailable-header"/>
                           </xsl:otherwise>
                        </xsl:choose>
                     </td>
                     <td class="chartexportbackground">
                        <span class="bold"><xsl:text>Ethnicity:</xsl:text></span>
                     </td>
                     <td class="chartexportbackground">
                        <xsl:choose>
                           <xsl:when test="n1:patient/n1:ethnicGroupCode/@displayName">
                              <xsl:for-each select="n1:patient/n1:ethnicGroupCode">
                                 <xsl:call-template name="show-race-ethnicity"/>
                              </xsl:for-each>
                           </xsl:when>
                           <xsl:otherwise>
                              <xsl:call-template name="show-unavailable-header"/>
                           </xsl:otherwise>
                        </xsl:choose>
                     </td>
                  </tr>
                  <tr>
                     <td class="chartexportbackground">
                        <span class="bold"><xsl:text>DOB:</xsl:text></span>
                     </td>
                     <td class="chartexportbackground">
                        <xsl:choose>
                           <xsl:when test="n1:patient/n1:birthTime">
                              <xsl:call-template name="show-date">
                                 <xsl:with-param name="datetime" select="n1:patient/n1:birthTime"/>
                              </xsl:call-template>
                           </xsl:when>
                           <xsl:otherwise>
                              <xsl:call-template name="show-unavailable-header"/>
                           </xsl:otherwise>
                        </xsl:choose>
                     </td>
                     <td class="chartexportbackground">
                        <span class="bold"><xsl:text>Race:</xsl:text></span>
                     </td>
                     <td class="chartexportbackground">
                        <xsl:choose>
                           <xsl:when test="n1:patient/n1:raceCode/@displayName">
                              <xsl:for-each select="n1:patient/n1:raceCode">
                                 <xsl:call-template name="show-race-ethnicity"/>
                              </xsl:for-each>
                              <xsl:for-each select="n1:patient/sdtc:raceCode">
                                 <xsl:text>, </xsl:text>
                                 <xsl:call-template name="show-race-ethnicity"/>
                              </xsl:for-each>
                           </xsl:when>
                           <xsl:otherwise>
                              <xsl:call-template name="show-unavailable-header"/>
                           </xsl:otherwise>
                        </xsl:choose>
                     </td>
                  </tr>
                  <tr>
                     <td class="chartexportbackground">
                        <span class="bold"><xsl:text>Preferred language:</xsl:text></span>
                     </td>
                     <td class="chartexportbackground">
                        <xsl:choose>
                           <xsl:when test="n1:patient/n1:languageCommunication/n1:languageCode">
                              <xsl:for-each select="n1:patient/n1:languageCommunication/n1:languageCode">
                        <xsl:call-template name="show-preferred-language"/>
                              </xsl:for-each>
                           </xsl:when>
                           <xsl:otherwise>
                              <xsl:call-template name="show-unavailable-header"/>
                           </xsl:otherwise>
                        </xsl:choose>
                     </td>
                     <td class="chartexportbackground">
                        <span class="bold"><xsl:text>Marital status:</xsl:text></span>
                     </td>
                     <td class="chartexportbackground">
                        <xsl:choose>
                           <xsl:when test="n1:patient/n1:maritalStatusCode/@displayName">
                              <xsl:value-of select="n1:patient/n1:maritalStatusCode/@displayName"/>
                           </xsl:when>
                           <xsl:otherwise>
                              <xsl:call-template name="show-unavailable-header"/>
                           </xsl:otherwise>
                        </xsl:choose>
                     </td>
                  </tr>
              <tr>
                     <td class="chartexportbackground">
                        <span class="bold"><xsl:text>Previous Name:</xsl:text></span>
                     </td>
                     <td class="chartexportbackground">
                        <xsl:call-template name="show-previous-name">
                           <xsl:with-param name="name" select="n1:patient/n1:name[position() != 1]"/>
                        </xsl:call-template>
                     </td>
              </tr>
                  <tr>
                     <td colspan="4">&#160;</td>
                  </tr>
                  <tr>
                     <td class="chartexportbackground">
                        <span class="bold"><xsl:text>Contact:</xsl:text></span>
                     </td>
                     <td class="chartexportbackground" colspan="3">
                        <xsl:call-template name="show-contactInfo">
                           <xsl:with-param name="contact" select="."/>
                           <xsl:with-param name="showphonetelecomcode" select="true()"/>
                           <xsl:with-param name="showaddresstelecomcode" select="false()"/>
                           <xsl:with-param name="showallnumbers" select="true()"/>
                        </xsl:call-template>
                     </td>
                  </tr>
                  <tr>
                     <td class="chartexportbackground">
                        <span class="bold"><xsl:text>Other Addresses:</xsl:text></span>
                     </td>
                     <td class="chartexportbackground" colspan="3">
                        <xsl:call-template name="show-contactInfo-otheraddresses">
                           <xsl:with-param name="contact" select="."/>
                        </xsl:call-template>
                     </td>
                  </tr>
               </xsl:for-each>
            </tbody>
         </table>
      </div>
   </xsl:template>
   <!-- showcareteamfunction -->
   <xsl:template name="showcareteamperson">
      <xsl:param name="islast"/>
      <xsl:param name="person"/>
      <tr>
         <!-- todo: add a td template to DRY this out -->
         <xsl:choose>
            <xsl:when test="$islast">
               <td class="chartexportbackground chartexportpadtop chartexportpadbot chartexportdashed">
                  <xsl:call-template name="show-assignedEntity">
                     <xsl:with-param name="asgnEntity" select="n1:assignedEntity"/>
                  </xsl:call-template>
               </td>
               <td class="chartexportbackground chartexportpadtop chartexportpadbot chartexportdashed">
                  <xsl:call-template name="show-contactInfo">
                     <xsl:with-param name="contact" select="n1:assignedEntity"/>
                     <xsl:with-param name="showphonetelecomcode" select="false()"/>
                     <xsl:with-param name="showaddresstelecomcode" select="false()"/>
                     <xsl:with-param name="showallnumbers" select="false()"/>
                  </xsl:call-template>
               </td>
            </xsl:when>
            <xsl:otherwise>
               <td class="chartexportbackground chartexportpadtop">
                  <xsl:call-template name="show-assignedEntity">
                     <xsl:with-param name="asgnEntity" select="n1:assignedEntity"/>
                  </xsl:call-template>
               </td>
               <td class="chartexportbackground chartexportpadtop">
                  <xsl:call-template name="show-contactInfo">
                     <xsl:with-param name="contact" select="n1:assignedEntity"/>
                     <xsl:with-param name="showphonetelecomcode" select="false()"/>
                     <xsl:with-param name="showaddresstelecomcode" select="false()"/>
                     <xsl:with-param name="showallnumbers" select="false()"/>
                  </xsl:call-template>
               </td>
            </xsl:otherwise>
         </xsl:choose>
      </tr>
   </xsl:template>
   <!-- careteammembertype -->
   <xsl:template name="careteammembertype">
      <xsl:param name="code"/>
      <xsl:param name="islast"/>
            <tr>
               <td width="30%" class="chartexportbackground chartexportpadtop">
                  <span class="bold"><xsl:value-of select="n1:functionCode/@displayName"/></span>
               </td>
               <td width="70%" class="chartexportbackground chartexportpadtop">
                  &#160;
               </td>
            </tr>
         <xsl:call-template name="showcareteamperson">
            <!-- Put a dashed line under the last member of this type unless it's the last type -->
            <xsl:with-param name="islast" select="not($islast) and position()=last()"/>
            <xsl:with-param name="person" select="$code"/>
         </xsl:call-template>
   </xsl:template>
   <!-- careteammembers -->
   <xsl:template name="careteammembers">
      <div class="ccdasection">
         <span class="chartexportsectionheader" section="Care Team Members">
            <a name="careteammembers">Care Team Members</a>
         </span>
         <table class="chartexporttable" cellSpacing="0">
            <tbody>
               <!--Loop through all care team members-->
               <xsl:for-each select="n1:documentationOf/n1:serviceEvent/n1:performer[n1:functionCode/@code!=0]">
                  <xsl:call-template name="careteammembertype">
                     <xsl:with-param name="code" select="."/>
                     <xsl:with-param name="islast" select="true()"/>   
                  </xsl:call-template>
               </xsl:for-each>
            </tbody>
         </table>
      </div>
   </xsl:template>
   <!-- LegalAuthenticator -->
   <xsl:template name="legalauthenticator">
      <div class="ccdasection">
         <span class="chartexportsectionheader" section="LegalAuthenticator">
            <a name="legalauthenticator">LegalAuthenticator</a>
         </span>
         <table class="chartexporttable" cellSpacing="0">
               <tbody>
                  <tr>
                     <td class="chartexportbackground">
                        <xsl:call-template name="show-assignedEntity">
                           <xsl:with-param name="asgnEntity" select="/n1:ClinicalDocument/n1:legalAuthenticator/n1:assignedEntity"/>
                        </xsl:call-template>
                        <xsl:if test="count(/n1:ClinicalDocument/n1:legalAuthenticator/n1:assignedEntity)!=0">
                           <xsl:text>: </xsl:text>
                        </xsl:if>
                        <xsl:call-template name="show-contactInfo">
                           <xsl:with-param name="contact" select="/n1:ClinicalDocument/n1:legalAuthenticator/n1:assignedEntity"/>
                           <xsl:with-param name="showphonetelecomcode" select="false()"/>
                           <xsl:with-param name="showaddresstelecomcode" select="false()"/>
                        </xsl:call-template>
                     </td>
                  </tr>
               </tbody>
         </table>
      </div>   
   </xsl:template>
   <!-- mostrecentencounter -->
   <xsl:template name="mostrecentencounter">
      <xsl:if test="count(//n1:encompassingEncounter/n1:effectiveTime)!=0 or count(//n1:encounterParticipant/n1:assignedEntity/n1:assignedPerson/n1:name) or count(//n1:encounterParticipant/n1:assignedEntity/n1:representedOrganization/n1:name) or count(//n1:encounterParticipant/n1:assignedEntity/n1:addr)!=0 or count(n1:encounterParticipant/n1:assignedEntity/n1:telecom)!=0">
         <div class="ccdasection">
            <span class="chartexportsectionheader" section="Most Recent Encounter">Encounter Details</span>
            <table class="chartexporttable" cellSpacing="0">
               <tbody>
		 <xsl:for-each select="//n1:encounterParticipant">
                  <xsl:if test="count(./n1:assignedEntity)!=0">
                     <tr>
                        <td class="chartexportbackground">
                           <xsl:call-template name="show-assignedEntity">
                              <xsl:with-param name="asgnEntity" select="./n1:assignedEntity"/>
                           </xsl:call-template>
                           <xsl:if test="count(./n1:assignedEntity)!=0 and (count(./n1:assignedEntity/n1:addr)!=0 or count(./n1:assignedEntity/n1:telecom)!=0)">
                              <xsl:text>: </xsl:text>
                           </xsl:if>
                           <xsl:call-template name="show-contactInfo">
                              <xsl:with-param name="contact" select="./n1:assignedEntity"/>
                              <xsl:with-param name="showphonetelecomcode" select="false()"/>
                              <xsl:with-param name="showaddresstelecomcode" select="false()"/>
                              <xsl:with-param name="showallnumbers" select="false()"/>
                           </xsl:call-template>
                        </td>
                     </tr>
                   </xsl:if>
		  </xsl:for-each>
                  <xsl:if test="count(//n1:encompassingEncounter/n1:effectiveTime)!=0">
                     <tr>
                        <td class="chartexportbackground">
                           <xsl:call-template name="show-admit-discharge-date">
                              <xsl:with-param name="datetime" select="//n1:encompassingEncounter/n1:effectiveTime"/>
                           </xsl:call-template>
                        </td>
                     </tr>
                  </xsl:if>
               </tbody>
            </table>
         </div>
      </xsl:if>
   </xsl:template>
   <!-- show StructuredBody  -->
   <xsl:template match="n1:component/n1:structuredBody">
      <!-- We are excluding the DS4P section from the usual StructuredBody template because we earlier
      pull it out and put it directly beneath the Table of Contents. -->
      <xsl:for-each select="n1:component/n1:section[not(.//*/@root = '2.16.840.1.113883.3.3251.1.5')]">
         <xsl:call-template name="section"/>
         <xsl:if test="position()!=last()">
            <hr/>
         </xsl:if>
      </xsl:for-each>
   </xsl:template>
   <!-- show nonXMLBody -->
   <xsl:template match='n1:component/n1:nonXMLBody'>
      <xsl:choose>
         <!-- if there is a reference, use that in an IFRAME -->
         <xsl:when test='n1:text/n1:reference'>
            <IFRAME name='nonXMLBody' id='nonXMLBody' WIDTH='80%' HEIGHT='600' src='{n1:text/n1:reference/@value}'/>
         </xsl:when>
         <xsl:when test='n1:text/@mediaType="text/plain"'>
            <pre>
               <xsl:value-of select='n1:text/text()'/>
            </pre>
         </xsl:when>
         <xsl:otherwise>
            <CENTER>Cannot display the text</CENTER>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>
   <!-- top level component/section: display title and text,
     and process any nested component/sections
   -->
   <xsl:template name="section">
      <div class="ccdasection">
         <xsl:call-template name="section-title">
            <xsl:with-param name="title" select="n1:title"/>
         </xsl:call-template>
         <!-- We want to exclude the author section specifically from the DS4P section. -->
         <xsl:if test="not(.//*/@root = '2.16.840.1.113883.3.3251.1.5')">
             <xsl:call-template name="section-author"/>
         </xsl:if>
         <xsl:call-template name="section-text"/>
         <xsl:for-each select="n1:component/n1:section">
            <xsl:call-template name="nestedSection">
               <xsl:with-param name="margin" select="2"/>
            </xsl:call-template>
         </xsl:for-each>
      </div>
   </xsl:template>
   <!-- top level section title -->
   <xsl:template name="section-title">
      <xsl:param name="title"/>
      <xsl:choose>
         <xsl:when test="count(/n1:ClinicalDocument/n1:component/n1:structuredBody/n1:component[n1:section]) &gt; 1">
            <span class="chartexportsectionheader" section="{$title}">
               <a name="{generate-id($title)}">
                  <xsl:value-of select="$title"/>
               </a>
            </span>
         </xsl:when>
         <xsl:otherwise>
            <span class="chartexportsectionheader" section="{$title}">
               <xsl:value-of select="$title"/>
            </span>
         </xsl:otherwise>
      </xsl:choose>
   </xsl:template>
   <!-- section author -->
   <xsl:template name="section-author">
      <xsl:if test="count(n1:author)&gt;0">
         <div style="margin-left : 2em;">
            <b>
               <xsl:text>Section Author: </xsl:text>
            </b>
            <xsl:for-each select="n1:author/n1:assignedAuthor">
               <xsl:choose>
                  <xsl:when test="n1:assignedPerson/n1:name">
                     <xsl:call-template name="show-name">
                        <xsl:with-param name="name" select="n1:assignedPerson/n1:name"/>
                     </xsl:call-template>
                     <xsl:if test="n1:representedOrganization">
                        <xsl:text>, </xsl:text>
                        <xsl:call-template name="show-name">
                           <xsl:with-param name="name" select="n1:representedOrganization/n1:name"/>
                        </xsl:call-template>
                     </xsl:if>
                  </xsl:when>
                  <xsl:when test="n1:assignedAuthoringDevice/n1:softwareName">
                     <xsl:value-of select="n1:assignedAuthoringDevice/n1:softwareName"/>
                  </xsl:when>
                  <xsl:otherwise>
                     <xsl:for-each select="n1:id">
                        <xsl:call-template name="show-id"/>
                        <br/>
                     </xsl:for-each>
                  </xsl:otherwise>
               </xsl:choose>
            </xsl:for-each>
            <br/>
         </div>
      </xsl:if>
   </xsl:template>
   <!-- top-level section Text   -->
   <xsl:template name="section-text">
     <xsl:if test="n1:text != ''">
         <div>
            <xsl:apply-templates select="n1:text"/>
         </div>
      </xsl:if>
   </xsl:template>
   <!-- nested component/section -->
   <xsl:template name="nestedSection">
      <xsl:param name="margin"/>
      <h4 style="margin-left : {$margin}em;">
         <xsl:value-of select="n1:title"/>
      </h4>
      <div style="margin-left : {$margin}em;">
         <xsl:apply-templates select="n1:text"/>
      </div>
      <xsl:for-each select="n1:component/n1:section">
         <xsl:call-template name="nestedSection">
            <xsl:with-param name="margin" select="2*$margin"/>
         </xsl:call-template>
      </xsl:for-each>
   </xsl:template>
   <!--   paragraph  -->
   <xsl:template match="n1:paragraph">
      <p>
         <xsl:call-template name="insertBreaks">
            <xsl:with-param name="lineBreak" select="1"/>
         </xsl:call-template>
      </p>
   </xsl:template>
   <xsl:template match="n1:subtitle">
      <b>
         <xsl:apply-templates/>
      </b>
   </xsl:template>
   <!--   pre format  -->
   <xsl:template match="n1:pre">
      <pre>
         <xsl:apply-templates/>
      </pre>
   </xsl:template>
   <!--   Content w/ deleted text is hidden -->
   <xsl:template match="n1:content[@revised='delete']"/>
   <!--   content  -->
   <xsl:template match="n1:content">
      <xsl:element name="span">
      <xsl:if test="@styleCode">
         <xsl:attribute name="class"><xsl:value-of select="./@styleCode"/></xsl:attribute>
      </xsl:if>
      <xsl:apply-templates/>
      </xsl:element>
   </xsl:template>
   <!-- line break -->
   <xsl:template match="n1:br">
      <xsl:element name='br'>
         <xsl:apply-templates/>
